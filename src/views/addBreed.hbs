    <main>
        <form action="/breeds" method="POST" class="cat-form" enctype="multipart/form-data">
            <h2>Add Cat Breed</h2>
            <label for="breed-name">Breed Name</label>
            <input name="breed" type="text" id="breed">
            <button type="submit">Add Breed</button>
        </form>
        <h3 class="message">{{err}}</h3>
    </main>
